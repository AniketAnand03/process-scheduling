	printf("\nEnter the arrival time and burst time of process\n");
	for(process_no=0;process_no<no;process_no++)
	{
		printf("\nProcess P%d\n",Process_no+1);
		printf("Arrival Time=");
		scanf("%d",&art[process_no]);
		printf("Burst Time=");
		scanf("%d",&bst[process_no]);
